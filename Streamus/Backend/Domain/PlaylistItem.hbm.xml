<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Streamus" namespace="Streamus.Backend.Domain">
  <class name="PlaylistItem" table="[PlaylistItems]" lazy="false" >
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="PlaylistId" not-null="true" />
    <property name="Position" not-null="true" />
    <property name="Title" not-null="true"/>
    <property name="VideoId" not-null="true"/>
    <property name="SongId" not-null="true"/>
  </class>
</hibernate-mapping>