<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Streamus" namespace="Streamus.Backend.Domain">
  <class name="Song" table="[Songs]" lazy="false">
    <id name="Id">
      <generator class="guid" />
    </id>

    <property name="VideoId" not-null="true" length="11"/>
    <property name="Title" not-null="true" />
    <property name="Duration" not-null="true" />
  </class>
</hibernate-mapping>